    <!ENTITY MULFXP_WORD_FORMAT '
    <widget id="MULFXP_WORD_FORMAT">
        <key>MULFXP_WORD_FORMAT</key>
        <label>32-bit fixed-point format</label>
        <tip>32-bit fixed-point format in Q notation, where the first number
        specifies the number of integer bits and the second number
        specifies the number of fractional bits.</tip>
    </widget>
    '>

